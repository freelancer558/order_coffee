<div class="well">
	<h4>All [<%= @users.size %>] users</h4>
  <table class="table table-bordered">
    <tr class="success" >
      <td>Photo</td>
      <td>Name</td>
      <td>e-mail</td>
      <td></td>
      <td></td>
      <td></td>
    </tr>
    <% @users.each do |user| %>
    <tr>
      <td><%= image_tag user.photo.url(:thumb), :width => 40, :class => 'img-polaroid pull-left' %></td>
      <td><%= user.username %></td>
      <td><%= user.email %></td>
      <td><%#= link_to 'Show', users %></td>
      <td><%#= link_to 'Edit', edit_admin_user_path(user) %></td>
      <td><%#= link_to 'Destroy', admin_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
    <% end %>
  </table>
</div>